    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.2",
    "@mui/x-date-pickers": "^8.11.2",
    "@toolpad/core": "^0.16.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
